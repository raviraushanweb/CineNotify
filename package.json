{
  "name": "cine-notify",
  "version": "1.0.0",
  "description": "CineNotify is a Telegram bot designed to keep movie enthusiasts informed about upcoming releases. Users can search for a movie, get release information, and add it to their reminder list.",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "server": "nodemon --exec ts-node src/server.ts",
    "dev": "npm run server",
    "clinic": "tsc && clinic doctor --autocannon [ / ] -- node dist/server.js",
    "bubbleprof_clinic": "tsc && clinic bubbleprof --autocannon [ / ] -- node dist/server.js",
    "flame_clinic": "tsc && clinic flame --autocannon [ / ] -- node dist/server.js",
    "heapprofiler_clinic": "tsc && clinic heapprofiler --autocannon [ / ] -- node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raviraushanweb/CineNotify.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "telegraf",
    "movie",
    "API",
    "mongoose",
    "MongoDB",
    "notifications",
    "reminders",
    "node.js",
    "TypeScript"
  ],
  "author": "Ravi Raushan <raviraushanweb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raviraushanweb/CineNotify/issues"
  },
  "homepage": "https://github.com/raviraushanweb/CineNotify#readme",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.1.4",
    "mongoose": "^7.3.0",
    "telegraf": "^4.12.2"
  }
}
